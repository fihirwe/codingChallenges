
public class F10001stPrime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int i=2;
		boolean prime=true;
		while (i<200) {
			prime = true;
	         for (int j = 2; j <= i/2; j++) {
	            if (i%j == 0) {
	               prime = false; 
	            }
	         }
	         if (prime)
	         System.out.print(" " + i);

	}


}
