import java.util.Iterator;

public class PythagoreanTriplet {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int i, j, k;
		for (i = 0; i < 1000; i++) {
			for (j = 0; j < 1000; j++) {
				for (k = 0; k < 1000; k++) {
					System.out.println("%");
				}
			}
		}
		System.out.println("Done");
	}

}
