
public class F10001stPrime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=2;
		boolean prime=true;
		while (n<200) {
			prime=true;
			int i;
			for(i=2;i<n/2;i++) {
				if(n/i==0) {
					prime=false;
				}

			}
			if(prime) {
				System.out.println(n);
			}
			n++;
		}

	}


}
